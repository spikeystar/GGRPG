config_version=4

_global_script_classes=[ {
"base": "Node2D",
"class": "TurnQueue",
"language": "GDScript",
"path": "res://Scripts/Battle/TurnQueue.gd"
} ]
_global_script_class_icons={
"TurnQueue": ""
}

[application]

config/name="Gary Goat RPG!"
run/main_scene="res://Areas/Pivot Town/Pivot_Town_1.tscn"
config/use_custom_user_dir=true
config/custom_user_dir_name="GGRPGSaveSystem"
config/icon="res://UI/Game Load Icon.png"
config/macos_native_icon="res://UI/Start Screen/Game Load Icon.ico"
config/windows_native_icon="res://UI/Start Screen/Game Load Icon.ico"

[autoload]

Global="*res://Scripts/Global.gd"
PlayerManager="*res://Scripts/Players/PlayerManager.gd"
Party="*res://Scripts/Players/Party.gd"
Music="*res://Misc/Music.tscn"
SE="*res://Misc/SE.tscn"
